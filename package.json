{
  "name": "@vnucka/vue3-date-range-picker",
  "version": "0.2.0",
  "description": "A date range picker",
  "author": "vnucka",
  "main": "dist/vue3-date-range-picker.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue3-date-range-picker src/components/DateRangePicker.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "moment": "^2.30.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.0",
    "@fortawesome/free-solid-svg-icons": "^6.5.0",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@vue/cli-plugin-babel": "^5.0.0",
    "@vue/cli-plugin-eslint": "^5.0.0",
    "@vue/cli-service": "^5.0.0",
    "@vue/eslint-config-standard": "^8.0.0",
    "babel-eslint": "^10.0.1",
    "bootstrap": "^5.3.0",
    "bootstrap-vue": "^2.23.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.22.0",
    "jquery": "^3.7.0",
    "vue": "^3.4.0",
    "vue-template-compiler": "^2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnucka/vue3-date-range-picker.git"
  },
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        "never"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
